{
    "name": "padosoft/laravel-notifier",
    "description": "Laravel Laravel Package for beautifull JS notification.",
    "keywords": [
        "padosoft",
        "laravel-notifier",
        "notifier",
        "notifier",
        "noty"
    ],
    "homepage": "https://github.com/padosoft/laravel-notifier",
    "license": "MIT",
    "authors": [
        {
            "name": "Lorenzo Padovani",
            "email": "helpdesk@padosoft.com",
            "homepage": "https://www.padosoft.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.1",
        "illuminate/support": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "illuminate/session": "^5.0|^6.0|^7.0|^8.0|^9.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "orchestra/testbench": "^3.2|^4.0|^5.0|^6.0|^7.0",
        "phpunit/phpunit": "^5.0|^6.0|^7.0|^8.0|^9.0",
        "mockery/mockery": "^0.9.9|^1.0",
        "padosoft/laravel-test": "^2.1",
        "padosoft/test": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "Padosoft\\Laravel\\Notification\\Notifier\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Padosoft\\Laravel\\Notification\\Notifier\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Padosoft\\Laravel\\Notification\\Notifier\\NotifierServiceProvider"
            ],
            "aliases": {
                "Notifier": "Padosoft\\Laravel\\Notification\\Notifier\\Facades\\Notifier"
            }
        }
    }
}
